from turtle import *
bgcolor("black")
up()
pensize(2)
begin_fill()
color("#027FC6")
goto(5,-200)
pendown()
circle(240)
end_fill()
up()
goto(-50,-100)
color("white")
pendown()
begin_fill()
circle(200,90)
right(70)
circle(50,40)
left(150)
circle(-35,40)
right(140)
circle(50,40)
left(150)
circle(-50,40)
right(60)
circle(60,160)
right(100)
circle(-150,60)
left(110)
circle(70,80)
left(230)
circle(-40,50)
left(130)
circle(60,85)
left(220)
circle(-30,60)
left(120)
circle(50,80)
right(125)
circle(-120,50)
left(150)
circle(197,41)
end_fill()
hideturtle()
up()
goto(-150,-350)
down()
pencolor("#027FC6")
write("Twitter", font = ("ariel",25,"bold"))
done()
