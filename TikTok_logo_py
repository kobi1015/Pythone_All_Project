from turtle import *
width(30)
bgcolor("black")
up()
goto(0,0)
down()
color("#db0f3c")
left(180)
circle(50,270)
forward(120)
left(180)
circle(50,90)
up()
goto(-5,13)
down()
color("#50ebe7")
left(180)
circle(50,270)
forward(120)
left(180)
circle(50,90)
up()
goto(-5,5)
down()
color("white")
left(180)
circle(50,270)
forward(120)
left(180)
circle(50,90)
done()
